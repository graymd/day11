<h1>This is where I'll keep my notes:</h1>

<%= link_to "New Note", new_note_path %>
<br>
<% @notes.each do |note| %>
	<br>
	<strong>Note Title:</strong>
	<%= link_to note.note_title, note_path(note) %>
	<br>
	<strong>Note:</strong>
	<br>
	<%= note.note_data %>
	<br>
	<%= link_to "Edit Note", edit_note_path(note) %>
	<%= link_to "Delete Note", note_path(note), method: :delete %>
	<br>
<% end %>
